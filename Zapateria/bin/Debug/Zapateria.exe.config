<configuration>
  <configSections>
  </configSections>
  <connectionStrings>
    <add name="cn" connectionString="server=localhost;user id=root;password=mik35563123;persistsecurityinfo=True;database=zapateria"
        providerName="MySql.Data.MySqlClient" />
    <add name="Agenda.My.MySettings.cn" connectionString="server=localhost;user id=root;password=mik35563123;persistsecurityinfo=True;database=agenda"
        providerName="MySql.Data.MySqlClient" />
